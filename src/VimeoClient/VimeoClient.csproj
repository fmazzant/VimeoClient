<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net47;net471;net472;net48;netstandard2.1;netstandard2.0;net5.0;net6.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Mafe.VimeoClient</PackageId>
    <Version>3.2.0-alpha4</Version>
    <Authors>Federico Mazzanti</Authors>
    <Company>Federico Mazzanti</Company>
    <Description>VimeoClient library provides a simple connector to Vimeo REST services.</Description>
    <Copyright>Copyright (c) 2020 Federico Mazzanti</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/fmazzant/VimeoClient</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fmazzant/VimeoClient</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>vimeo, vimeo-client, c-sharp, dotnet-standard, http-client, restclient, mafe, json, rest, fluent</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <AssemblyName>Mafe.$(MSBuildProjectName)</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\README.md" Link="README.md" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Body\Category\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Mafe.RestClient" Version="1.9.1" />
  </ItemGroup>
</Project>
